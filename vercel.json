{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "sales-feed.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/sales-feed",
      "dest": "/sales-feed.js"
    },
    {
      "src": "/api/sales-feed",
      "dest": "/sales-feed.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
